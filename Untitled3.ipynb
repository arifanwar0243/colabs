    "import numpy as np\n",
    "\n",
    "theta = np.radians(90)\n",
    "p1 = np.array([[4], [6]])\n",
    "R10 = np.array([[np.cos(theta), -np.sin(theta)], [np.sin(theta), np.cos(theta)]])\n",
    "d10 = np.array([[-3], [0]])\n",
    "\n",
    "p0 = R10 @ p1 + d10\n",
    "p0"
   ]
